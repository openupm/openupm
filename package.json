    "eslint-plugin-vue": "^6.2.2",
    "jquery": "^3.5.0",
    "js-yaml": "^3.13.1",
    "js-yaml-loader": "^1.2.2",
    "marked": "^0.8.0",
    "mocha": "^7.0.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "rewire": "^4.0.1",
